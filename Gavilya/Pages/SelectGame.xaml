<Page x:Class="Gavilya.Pages.SelectGame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:lang="clr-namespace:Gavilya.Properties"
      xmlns:local="clr-namespace:Gavilya.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SelectGame">

    <Grid>
        <TextBlock Margin="0 0 0 50" Text="{x:Static lang:Resources.SelectGameStart}" Foreground="White" FontSize="25" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Grid>
</Page>
